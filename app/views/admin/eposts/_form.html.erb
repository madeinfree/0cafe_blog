<h3>日記</h3>
<%= form_for [:admin, @epost] do |f| %>
  Title: <%= f.text_field :title %>
  Tag: <%= f.text_field :tag %><br>
  <%= f.text_area :content, size: "120x20", :class => 'form-control', :'data-uk-htmleditor' => "{markdown:true,mode:'tab'}" %><br>
  <%= f.submit 'sumbit' %>
<span style="float:right"><%= link_to('Back',admin_eposts_url) %></span>
<% end %>
